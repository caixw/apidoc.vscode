{
    "name": "apidoc",
    "displayName": "apidoc",
    "description": "%description%",
    "version": "7.1.0",
    "publisher": "caixw",
    "preview": true,
    "author": {
        "name": "caixw",
        "url": "https://github.com/caixw"
    },
    "keywords": [
        "apidoc",
        "restful",
        "api",
        "documents",
        "tools"
    ],
    "license": "MIT",
    "homepage": "https://apidoc.tools",
    "bugs": {
        "url": "https://github.com/caixw/apidoc.vscode/issues",
        "email": "xnotepad@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/caixw/apidoc.vscode.git"
    },
    "engines": {
        "vscode": "^1.46.1"
    },
    "galleryBanner": {
        "color": "#5c2d91",
        "theme": "dark"
    },
    "icon": "resources/icon.png",
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:apidoc",
        "onLanguage:csharp",
        "onLanguage:c",
        "onLanguage:cpp",
        "onLanguage:d",
        "onLanguage:dart",
        "onLanguage:erlang",
        "onLanguage:go",
        "onLanguage:groovy",
        "onLanguage:java",
        "onLanguage:javascript",
        "onLanguage:kotlin",
        "onLanguage:lua",
        "onLanguage:lisp",
        "onLanguage:pascal",
        "onLanguage:perl",
        "onLanguage:php",
        "onLanguage:python",
        "onLanguage:ruby",
        "onLanguage:rust",
        "onLanguage:scala",
        "onLanguage:swift",
        "onLanguage:typescript",
        "onLanguage:zig"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "%config-title%",
            "properties": {
                "conf.cmd": {
                    "type": "string",
                    "default": "",
                    "description": "%config-cmd-desc%"
                },
                "conf.port": {
                    "type": "string",
                    "default": ":8080",
                    "description": "%config-port-desc%"
                }
            }
        },
        "languages": [
            {
                "id": "apidoc",
                "aliases": [
                    "APIDOC"
                ],
                "filenames": [
                    "apidoc.xml"
                ],
                "mimetypes": [
                    "application/xml",
                    "text/xml"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "gulp compile",
        "compile": "gulp compile",
        "watch": "gulp watch",
        "pretest": "gulp compile",
        "test": "node ./out/test/runTest.js",
        "clean": "gulp clean",
        "package": "gulp package",
        "publish": "gulp publish"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^8.0.0",
        "@types/node": "^10.12.21",
        "@types/semver": "^7.3.1",
        "@types/vscode": "^1.46.1",
        "@types/which": "^1.3.2",
        "del": "^5.1.0",
        "glob": "^7.1.4",
        "gulp": "^4.0.2",
        "gulp-cached": "^1.1.1",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-typescript": "^5.0.1",
        "mocha": "^8.0.1",
        "tslint": "^6.1.2",
        "typescript": "^3.8.3",
        "vsce": "^1.66.0",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "semver": "^7.3.2",
        "vscode-languageclient": "^6.1.3",
        "which": "^2.0.2"
    }
}
