{
	"name": "apidoc",
	"displayName": "apidoc",
	"description": "%description%",
	"version": "0.0.1",
	"publisher": "caixw",
	"preview": true,
	"author": {
		"name": "caixw",
		"url": "https://github.com/caixw"
	},
	"keywords": [
		"apidoc",
		"restful",
		"api",
		"documents",
		"tools"
	],
	"license": "MIT",
	"homepage": "https://apidoc.tools",
	"bugs": {
		"url": "https://github.com/caixw/apidoc.vscode/issues",
		"email": "xnotepad@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/caixw/apidoc.vscode.git"
	},
	"engines": {
		"vscode": "^1.36.1"
	},
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	},
	"icon": "resources/icon.png",
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "apidoc",
				"aliases": [
					"APIDOC"
				],
				"extensions": [
					".apidoc"
				],
				"mimetypes": [
					"application/xml",
					"text/xml"
				]
			}
		],
		"grammars": [
			{
				"language": "apidoc",
				"scopeName": "text.xml.apidoc",
				"path": "./syntaxes/apidoc.tmLanguage.json",
				"injectTo": [
					"source.cs",
					"source.c",
					"source.cpp",
					"source.d",
					"source.erlang",
					"source.go",
					"source.groovy",
					"source.java",
					"source.js",
					"source.kotlin",
					"source.pascal",
					"source.perl",
					"source.php",
					"source.python",
					"source.ruby",
					"source.rust",
					"source.scala",
					"source.swift"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "gulp compile",
		"compile": "gulp compile",
		"watch": "gulp watch",
		"pretest": "gulp compile",
		"test": "node ./out/test/runTest.js",
		"clean": "gulp clean",
		"package": "gulp package",
		"publish": "gulp publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"del": "^5.1.0",
		"glob": "^7.1.4",
		"gulp": "^4.0.2",
		"gulp-cached": "^1.1.1",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^5.0.1",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.6.2",
		"vsce": "^1.66.0",
		"vscode-test": "^1.0.2"
	}
}
